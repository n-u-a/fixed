{
  "manifest_version": 2,
  "version": "1.0.2",
  "name": "__MSG_extName__",
  "description": "You can view the list of commits on the pull request and copy any commit ID.",
  "default_locale": "en",
  "permissions": [
    "https://github.com/",
    "tabs"
  ],
  "icons": {
    "16": "icons/clip_commit_ver2.png",
    "38": "icons/clip_commit_ver2.png",
    "128": "icons/clip_commit_ver2.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "__MSG_extName__"
  },
  "content_security_policy":"script-src 'self' 'unsafe-eval' https://*.firebaseio.com https://www.googleapis.com https://apis.google.com https://*.firebaseapp.com; object-src 'self'",
  "key":""
}